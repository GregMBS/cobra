{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=8.0",
        "ext-PDO": "*",
        "ext-fileinfo": "*",
        "ext-json": "*",
        "barryvdh/laravel-ide-helper": "*",
        "box/spout": "*",
        "fideloper/proxy": "*",
        "filp/whoops": "*",
        "fzaninotto/faker": "*",
        "laravel/framework": "*",
        "laravel/helpers": "*",
        "laravel/homestead": "*",
        "laravel/tinker": "*",
        "laravelcollective/html": "*",
        "maatwebsite/excel": "*",
        "rcrowe/twigbridge": "*",
        "spatie/laravel-medialibrary": "*",
        "twig/twig": "3.14.1.0"
    },
    "require-dev": {
        "laravel/dusk": "*",
        "mockery/mockery": "*",
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
        "post-create-project-cmd": "@php artisan key:generate",
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
